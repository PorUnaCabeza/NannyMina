.apot-modal {
  position: absolute;
  z-index: 102;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  background-color: #fff;
}
.apot-modal .list {
  padding: 0 20rpx;
}
.apot-modal .list .item {
  display: flex;
  justify-content: flex-start;
  align-items: center;
  padding: 30rpx 0;
  border-bottom: 1rpx solid #f0f0f0;
}
.apot-modal .list .item .iconfont {
  font-size: 40rpx;
  color: #e5e5e5;
  margin-right: 16rpx;
}
.apot-modal .list .item .iconfont.active {
  color: #f4817c;
}
.apot-modal .list .item .content .name-line {
  display: flex;
  align-items: center;
}
.apot-modal .list .item .content .name-line .name {
  font-size: 32rpx;
  color: #000000;
  font-weight: bold;
}
.apot-modal .list .item .content .name-line .phone {
  margin-left: 16rpx;
  font-size: 28rpx;
  color: #434343;
}
.apot-modal .list .item .address {
  margin-top: 10rpx;
  font-size: 28rpx;
  color: #434343;
}
.apot-modal .confirm {
  position: fixed;
  left: 0;
  right: 0;
  width: 100%;
  bottom: 0;
  background-color: #f4817c;
  height: 90rpx;
  line-height: 90rpx;
  text-align: center;
  color: #fff;
  font-size: 32rpx;
}
